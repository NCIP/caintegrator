<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

    <package name="caintegrator2" extends="struts-default">

        <result-types>
            <result-type name="tiles" class="org.apache.struts2.views.tiles.TilesResult"/>
        </result-types>
       
        <default-interceptor-ref name="paramsPrepareParamsStack" />
        
        <!--  Navigation Actions -->
        <action name="home" class="homepageAction">
            <result type="tiles">homepage</result>
        </action>
        
        <action name="login" method="openLogin" class="loginAction">
            <result name="login" type="tiles">login</result>
            
        </action>
        
        <action name="workspace" method="openWorkspace" class="workspaceAction">
            <result name="workspaceStudy" type="tiles">workspaceStudy</result>
            <result name="workspaceNoStudy">workspaceNoStudy.jsp</result>
        </action>
        
        
        <!--  Study Actions -->
   
        <action name="manageStudies" class="manageStudiesAction">
            <result type="tiles">manageStudies</result>
        </action>

        <action name="createStudy" class="editStudyAction">
            <result type="tiles">editStudy</result>
        </action>
            
        <action name="deployStudy" class="deployStudyAction">
            <result type="redirectAction">manageStudies</result>
        </action>
      
        <action name="editStudy" class="editStudyAction">
            <result type="tiles">editStudy</result>
        </action>
   
        <action name="saveStudy" class="saveStudyAction">
            <result type="tiles">editStudy</result>
            <result name="input" type="tiles">editStudy</result>
        </action>
   
        <action name="addClinicalFile" class="addClinicalFileAction">
            <result type="tiles">editClinicalFile</result>
            <result name="input" type="tiles">editStudy</result>
        </action>
       
        <action name="editClinicalSource" class="editClinicalSourceAction">
            <result type="tiles">editClinicalFile</result>
        </action>
       
       <action name="editImagingSource" class="editImagingSourceAction">
            <result type="tiles">editImagingFile</result>
        </action>
        
        <action name="saveClinicalSource" class="saveClinicalSourceAction">
            <result type="tiles">editStudy</result>
        </action>
                      
        <action name="loadClinicalSource" class="loadClinicalSourceAction">
            <result type="tiles">editStudy</result>
        </action>
        
        <action name="createNewDefinition" method="createNewDefinition" class="defineFileColumnAction">
            <result type="tiles">editFileColumn</result>
        </action>
        
        <action name="editFileColumn" method="editFileColumn" class="defineFileColumnAction">
            <result type="tiles">editFileColumn</result>
        </action>
   
         <action name="editImagingFileColumn" method="editFileColumn" class="defineImagingFileColumnAction">
            <result type="tiles">editImagingFileColumn</result>
        </action>
        
        <action name="searchDefinitions" method="searchDefinitions" class="defineFileColumnAction">
            <result type="tiles">editFileColumn</result>
        </action>
        
        <action name="saveColumnType" method="saveColumnType" class="defineFileColumnAction">
            <result type="tiles">editFileColumn</result>
        </action>
        
        <action name="searchImagingDefinitions" method="searchDefinitions" class="defineImagingFileColumnAction">
            <result type="tiles">editImagingFileColumn</result>
        </action>
   
        <action name="selectDataElement" method="selectDataElement" class="defineFileColumnAction">
            <result type="tiles">editFileColumn</result>
        </action>
   
        <action name="selectImagingDataElement" method="selectDataElement" class="defineImagingFileColumnAction">
            <result type="tiles">editImagingFileColumn</result>
        </action>
        
        <action name="selectDefinition" method="selectDefinition" class="defineFileColumnAction">
            <result type="tiles">editFileColumn</result>
        </action>
   
        <action name="selectImagingDefinition" method="selectDefinition" class="defineImagingFileColumnAction">
            <result type="tiles">editImagingFileColumn</result>
        </action>
        
        <action name="updateFileColumn" method="updateFileColumn" class="defineFileColumnAction">
            <result type="redirectAction">
                <param name="actionName">editClinicalSource</param>
                <param name="studyConfiguration.id">${studyConfiguration.id}</param>
                <param name="clinicalSource.id">${clinicalSource.id}</param>
            </result>
        </action>
        
        <action name="updateImagingFileColumn" method="updateFileColumn" class="defineImagingFileColumnAction">
            <result type="redirectAction">
                <param name="actionName">editImagingSource</param>
                <param name="studyConfiguration.id">${studyConfiguration.id}</param>
                <param name="imagingSource.id">${imagingSource.id}</param>
            </result>
        </action>

        <action name="addGenomicSource" class="editGenomicSourceAction">
            <result type="tiles">editGenomicSource</result>
        </action>
        
        <action name="editGenomicSource" class="editGenomicSourceAction">
            <result type="tiles">editGenomicSource</result>
        </action>
        
        <action name="saveGenomicSource" class="saveGenomicSourceAction">
            <result type="tiles">editStudy</result>
            <result name="input" type="tiles">editGenomicSource</result>
        </action>
        
        <action name="editSampleMapping" class="editSampleMappingAction">
            <result type="tiles">editSampleMapping</result>
            <result name="input" type="tiles">editGenomicSource</result>
        </action>
        
        <action name="saveSampleMapping" class="saveSampleMappingAction">
            <result type="tiles">editStudy</result>
            <result name="input" type="tiles">editSampleMapping</result>
        </action>
        
        <action name="addImagingSource" class="editImagingSourceAction">
            <result type="tiles">editImagingSource</result>
        </action>
        
        <action name="addImagingFile" class="addImagingFileAction">
            <result type="tiles">editImagingFile</result>
            <result name="input" type="tiles">editImagingSource</result>
        </action>
        
        
        
        <action name="loadImagingSource" class="loadImagingSourceAction">
            <result type="tiles">editStudy</result>
        </action>
        
        <action name="saveImagingSource" class="saveImagingSourceAction">
            <result type="tiles">editStudy</result>
        </action>
          
        <action name="editQuery" class="editQueryAction">
            <result type="tiles">editQuery</result>
        </action>
        
    </package>
    
</struts>
