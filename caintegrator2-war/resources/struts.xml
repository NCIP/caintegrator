<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

    <package name="caintegrator2" extends="struts-default">

        <result-types>
            <result-type name="tiles" class="org.apache.struts2.views.tiles.TilesResult"/>
            <result-type name="kmPlotResult" class="gov.nih.nci.caintegrator2.web.action.analysis.KMPlotResult"/>
            <result-type name="redirect" class="org.apache.struts2.dispatcher.ServletRedirectResult"/>
        </result-types>
        
        
        <default-interceptor-ref name="paramsPrepareParamsStack" />
        
        <!--  Navigation Actions -->
        <action name="notYetImplemented" class="emptyAction">
            <result type="tiles">notYetImplemented</result>
        </action>
        
        <action name="home" class="homepageAction">
            <result type="tiles">homepage</result>
        </action>
        
        <action name="browseStudy" class="browseStudyAction">
            <result type="tiles">browseStudy</result>
        </action>
        
        <action name="login" method="openLogin" class="loginAction">
            <result name="login" type="tiles">login</result>
            
        </action>
        
        <action name="workspace" method="openWorkspace" class="workspaceAction">
            <result name="workspaceStudy" type="redirectAction">
                <param name="actionName">browseStudy</param>
            </result>
            <result name="workspaceNoStudy" type="tiles">homepage</result>
        </action>
        
        <!--  Study Actions -->
   
        <action name="manageStudies" class="manageStudiesAction">
            <result type="tiles">manageStudies</result>
        </action>

        <action name="createStudy" class="editStudyAction">
            <result type="tiles">editStudy</result>
        </action>
            
        <action name="deployStudy" class="deployStudyAction">
            <result type="redirectAction">manageStudies</result>
            <result name="error" type="tiles">editStudy</result>
        </action>
      
        <action name="editStudy" class="editStudyAction">
            <result type="tiles">editStudy</result>
        </action>
      
        <action name="deleteStudy" method="deleteStudy" class="editStudyAction">
            <result type="redirectAction">manageStudies</result>
        </action>
   
        <action name="saveStudy" class="saveStudyAction">
            <result type="tiles">editStudy</result>
            <result name="input" type="tiles">editStudy</result>
        </action>
   
        <action name="addStudyLogo" class="addStudyLogoAction">
        
             <interceptor-ref name="paramsPrepareParamsStack">
                 <param name="fileUpload.allowedTypes">image/pjpeg,image/jpeg,image/gif</param>
             </interceptor-ref>
		    
            <result type="tiles">editStudy</result>
            <result name="input" type="tiles">editStudy</result>
            <result name="error" type="tiles">editStudy</result>
        </action>
        
        <action name="addClinicalFile" class="addClinicalFileAction">
            <result type="tiles">editClinicalFile</result>
            <result name="input" type="tiles">editStudy</result>
        </action>
       
        <action name="editClinicalSource" class="editClinicalSourceAction">
            <result type="tiles">editClinicalFile</result>
        </action>
       
       <action name="editImagingSource" class="editImagingSourceAction">
            <result type="tiles">editImagingFile</result>
        </action>
        
        <action name="saveClinicalSource" class="saveClinicalSourceAction">
            <result type="tiles">editStudy</result>
        </action>
                      
        <action name="loadClinicalSource" class="loadClinicalSourceAction">
            <result type="tiles">editStudy</result>
            <result name="input" type="tiles">editStudy</result>
        </action>
        
        <action name="createNewDefinition" method="createNewDefinition" class="defineFileColumnAction">
            <result type="tiles">editFileColumn</result>
            <result name="input" type="tiles">editFileColumn</result>
            <result name="error" type="tiles">editFileColumn</result>
        </action>
        
        <action name="createNewImagingDefinition" method="createNewDefinition" class="defineImagingFileColumnAction">
            <result type="tiles">editImagingFileColumn</result>
            <result name="input" type="tiles">editImagingFileColumn</result>
            <result name="error" type="tiles">editImagingFileColumn</result>
        </action>
        
        <action name="editSurvivalValueDefinitions" method="editSurvivalValueDefinitions" class="defineSurvivalDefinitionAction">
            <result type="tiles">editSurvivalValueDefinitions</result>
        </action>
        
        <action name="editSurvivalValueDefinition" method="editSurvivalValueDefinition" class="defineSurvivalDefinitionAction">
            <result type="tiles">editSurvivalValueDefinitions</result>
            <result name="input" type="tiles">editSurvivalValueDefinitions</result>
        </action>
        
        <action name="newSurvivalValueDefinition" method="newSurvivalValueDefinition" class="defineSurvivalDefinitionAction">
            <result type="tiles">editSurvivalValueDefinitions</result>
        </action>
        
        <action name="deleteSurvivalValueDefinition" method="deleteSurvivalValueDefinition" class="defineSurvivalDefinitionAction">
            <result type="tiles">editSurvivalValueDefinitions</result>
            <result name="input" type="tiles">editSurvivalValueDefinitions</result>
        </action>
        
        <action name="saveSurvivalValueDefinition" method="saveSurvivalValueDefinition" class="defineSurvivalDefinitionAction">
            <result type="redirectAction">
                <param name="actionName">editStudy</param>
                <param name="studyConfiguration.id">${studyConfiguration.id}</param>
            </result>
        </action>
        
        <action name="editFileColumn" method="editFileColumn" class="defineFileColumnAction">
            <result type="tiles">editFileColumn</result>
            <result name="input" type="tiles">editFileColumn</result>
            <result name="error" type="tiles">editFileColumn</result>
        </action>
   
         <action name="editImagingFileColumn" method="editFileColumn" class="defineImagingFileColumnAction">
            <result type="tiles">editImagingFileColumn</result>
            <result name="input" type="tiles">editImagingFileColumn</result>
            <result name="error" type="tiles">editImagingFileColumn</result>
        </action>
        
        <action name="searchDefinitions" method="searchDefinitions" class="defineFileColumnAction">
            <result type="tiles">editFileColumn</result>
            <result name="input" type="tiles">editFileColumn</result>
            <result name="error" type="tiles">editFileColumn</result>
        </action>
        
        <action name="saveColumnType" method="saveColumnType" class="defineFileColumnAction">
            <result type="tiles">editFileColumn</result>
            <result name="input" type="tiles">editFileColumn</result>
            <result name="error" type="tiles">editFileColumn</result>
        </action>
        
        <action name="saveImagingColumnType" method="saveColumnType" class="defineImagingFileColumnAction">
            <result type="tiles">editImagingFileColumn</result>
            <result name="input" type="tiles">editImagingFileColumn</result>
            <result name="error" type="tiles">editImagingFileColumn</result>
        </action>
        
        <action name="searchImagingDefinitions" method="searchDefinitions" class="defineImagingFileColumnAction">
            <result type="tiles">editImagingFileColumn</result>
            <result name="input" type="tiles">editImagingFileColumn</result>
            <result name="error" type="tiles">editImagingFileColumn</result>
        </action>
   
        <action name="selectDataElement" method="selectDataElement" class="defineFileColumnAction">
            <result type="redirectAction">
                <param name="actionName">editClinicalSource</param>
                <param name="studyConfiguration.id">${studyConfiguration.id}</param>
                <param name="clinicalSource.id">${clinicalSource.id}</param>
            </result>
            <result name="input" type="tiles">editFileColumn</result>
            <result name="error" type="tiles">editFileColumn</result>
        </action>
   
        <action name="selectImagingDataElement" method="selectDataElement" class="defineImagingFileColumnAction">
            <result type="redirectAction">
                <param name="actionName">editImagingSource</param>
                <param name="studyConfiguration.id">${studyConfiguration.id}</param>
                <param name="imagingSource.id">${imagingSource.id}</param>
            </result>
            <result name="input" type="tiles">editImagingFileColumn</result>
            <result name="error" type="tiles">editImagingFileColumn</result>
        </action>
        
        <action name="selectDefinition" method="selectDefinition" class="defineFileColumnAction">
            <result type="redirectAction">
                <param name="actionName">editClinicalSource</param>
                <param name="studyConfiguration.id">${studyConfiguration.id}</param>
                <param name="clinicalSource.id">${clinicalSource.id}</param>
            </result>
            <result name="input" type="tiles">editFileColumn</result>
            <result name="error" type="tiles">editFileColumn</result>
        </action>
   
        <action name="selectImagingDefinition" method="selectDefinition" class="defineImagingFileColumnAction">
            <result type="redirectAction">
                <param name="actionName">editImagingSource</param>
                <param name="studyConfiguration.id">${studyConfiguration.id}</param>
                <param name="imagingSource.id">${imagingSource.id}</param>
            </result>
            <result name="input" type="tiles">editImagingFileColumn</result>
            <result name="error" type="tiles">editImagingFileColumn</result>
        </action>
        
        <action name="updateFileColumn" method="updateFileColumn" class="defineFileColumnAction">
            <result type="redirectAction">
                <param name="actionName">editClinicalSource</param>
                <param name="studyConfiguration.id">${studyConfiguration.id}</param>
                <param name="clinicalSource.id">${clinicalSource.id}</param>
            </result>
            <result name="input" type="tiles">editFileColumn</result>
            <result name="error" type="tiles">editFileColumn</result>
        </action>

        
        <action name="updateImagingFileColumn" method="updateFileColumn" class="defineImagingFileColumnAction">
            <result type="redirectAction">
                <param name="actionName">editImagingSource</param>
                <param name="studyConfiguration.id">${studyConfiguration.id}</param>
                <param name="imagingSource.id">${imagingSource.id}</param>
            </result>
            <result name="input" type="tiles">editImagingFileColumn</result>
            <result name="error" type="tiles">editImagingFileColumn</result>
        </action>
        
        <action name="homepage.getStudyDetails" method="getStudyDetails" class="homepageAction">
            <result type="redirectAction">
                <param name="actionName">browseStudy</param>
                <param name="studyConfiguration.id">${studyConfiguration.id}</param>
            </result>
        </action>
        <action name="addGenomicSource" class="editGenomicSourceAction">
            <result type="tiles">editGenomicSource</result>
        </action>
        
        <action name="editGenomicSource" class="editGenomicSourceAction">
            <result type="tiles">editGenomicSource</result>
        </action>
        
        <action name="saveGenomicSource" class="saveGenomicSourceAction">
            <result type="tiles">editStudy</result>
            <result name="input" type="tiles">editGenomicSource</result>
        </action>
        
        <action name="editSampleMapping" class="editSampleMappingAction">
            <result type="tiles">editSampleMapping</result>
            <result name="input" type="tiles">editGenomicSource</result>
        </action>
        
        <action name="saveControlSamples" class="saveControlSamplesAction">
            <result type="tiles">editStudy</result>
            <result name="input" type="tiles">editSampleMapping</result>
        </action>

        <action name="saveSampleMapping" class="saveSampleMappingAction">
            <result type="tiles">editStudy</result>
            <result name="input" type="tiles">editSampleMapping</result>
        </action>
        
        <action name="addImagingSource" class="editImagingSourceAction">
            <result type="tiles">editImagingSource</result>
        </action>
        
        <action name="addImagingFile" class="addImagingFileAction">
            <result type="tiles">editImagingFile</result>
            <result name="input" type="tiles">editImagingSource</result>
        </action>
        
        <action name="loadImagingSource" class="loadImagingSourceAction">
            <result type="tiles">editStudy</result>
            <result name="input" type="tiles">editStudy</result>
        </action>
        
        <action name="saveImagingSource" class="saveImagingSourceAction">
            <result type="tiles">editStudy</result>
        </action>
                
        <!--  Query Actions -->
        
        <action name="newQuery" class="newQueryAction">
            <result type="redirectAction">
                <param name="actionName">manageQuery</param>
                <param name="selectedAction">createNewQuery</param>
            </result>
            <result name="input" type="tiles">homepage</result>
        </action>
        
        <action name="manageQuery" class="manageQueryAction">
            <result type="tiles">manageQuery</result>
            <result name="input" type="tiles">manageQuery</result>
            <result name="error" type="tiles">manageQuery</result>
            <result name="nciaBasket" type="redirect">${displayableWorkspace.nciaBasket.nciaBasketUrl} </result>
            <result name="export" type="tiles">exportSearchResults</result>
        </action>
        
        <action name="selectQueryTab" class="selectQueryTabAction">
            <result name="selectedTabCriteria">/WEB-INF/jsp/tabs/editQueryCriteria.jsp</result>
            <result name="selectedTabColumns">/WEB-INF/jsp/tabs/editQueryColumns.jsp</result>
            <result name="selectedTabSorting">/WEB-INF/jsp/tabs/editQuerySorting.jsp</result>
            <result name="selectedTabSearchResults">/WEB-INF/jsp/tabs/editQuerySearchResults.jsp</result>
            <result name="selectedTabSaveAs">/WEB-INF/jsp/tabs/editQuerySaveAs.jsp</result>
        </action>

        <!--  Analysis Actions -->
   
        <action name="genePatternAnalysis" class="genePatternAnalysisAction">
            <result type="tiles">genePatternAnalysis</result>
            <result name="results" type="tiles">genePatternAnalysisResults</result>
            <result name="error" type="tiles">genePatternAnalysis</result>
            <result name="input" type="tiles">genePatternAnalysis</result>
        </action>
        
        <!-- Kaplan-Meier Actions -->
        <action name="retrieveKMPlot" method="retrievePlot" class="kmPlotAnnotationBasedAction">
            <result name="kmPlotResult" type="kmPlotResult"/>
        </action>
        
        <!-- Annotation Based Kaplan-Meier Actions -->
        <action name="kmPlotAnnotationBasedInput" method="input" class="kmPlotAnnotationBasedAction">
            <result type="tiles">kaplanMeierAnnotationBased</result>
            <result name="input" type="tiles">kaplanMeierAnnotationBased</result>
        </action>
        
        <action name="kmPlotUpdateAnnotationDefinitions" method="updateAnnotationDefinitions" class="kmPlotAnnotationBasedAction">
            <result type="tiles">kaplanMeierAnnotationBased</result>
            <result name="input" type="tiles">kaplanMeierAnnotationBased</result>
        </action>
        
         <action name="kmPlotUpdatePermissibleValues" method="updatePermissibleValues" class="kmPlotAnnotationBasedAction">
            <result type="tiles">kaplanMeierAnnotationBased</result>
            <result name="input" type="tiles">kaplanMeierAnnotationBased</result>
        </action>
        
         <action name="createAnnotationBasedKMPlot" method="createPlot" class="kmPlotAnnotationBasedAction">
            <result type="tiles">kaplanMeierAnnotationBased</result>
            <result name="input" type="tiles">kaplanMeierAnnotationBased</result>
        </action>
        

    </package>
    
</struts>
