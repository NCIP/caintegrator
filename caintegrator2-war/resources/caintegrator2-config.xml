        <property name="genePatternGridClientFactory">
            <ref local="genePatternGridClientFactory" />
        </property>
        <property name="mbaGenerator">
            <ref local="mbaGenerator" />
        </property>
        <property name="queryManagementService">
            <ref local="queryManagementService" />
        </property>
        <property name="fileManager">
            <ref local="fileManager" />
        </property>
    </bean>
    
    <bean id="genePatternGridClientFactory" class="gov.nih.nci.caintegrator2.application.analysis.GenePatternGridClientFactoryImpl" />

    <bean id="mbaGenerator" class="edu.columbia.geworkbench.cagrid.MageBioAssayGeneratorImpl" />

    <bean id="fileManager"
        class="gov.nih.nci.caintegrator2.file.FileManagerImpl">
        <property name="configurationHelper">
            <ref local="configurationHelper" />
        </property>
    </bean>
    
    <bean id="logoRenderingServlet" 
        class="gov.nih.nci.caintegrator2.web.LogoRenderingServlet">
        <property name="studyManagementService">
            <ref local="studyManagementService"/>
        </property>
    </bean>

    <!-- ******************************************************************* -->
    <!-- ** SERVICES                                                      ** -->
    <!-- ******************************************************************* -->

    <bean id="jmsConnectionFactory" class="org.springframework.jndi.JndiObjectFactoryBean">
        <property name="jndiName" value="java:ConnectionFactory" />
    </bean>
    
    <bean id="platformLoaderQueue" class="org.springframework.jndi.JndiObjectFactoryBean">
        <property name="jndiName" value="queue/platformLoaderQueue" />
    </bean>
    
    <bean id="platformLoaderJmsTemplate" class="org.springframework.jms.core.JmsTemplate">
        <property name="connectionFactory" ref="jmsConnectionFactory" />
        <property name="defaultDestination" ref="platformLoaderQueue" />
        <property name="receiveTimeout" value="1" />
    </bean>

    <bean id="platformLoaderListenerContainer" class="org.springframework.jms.listener.DefaultMessageListenerContainer">
        <property name="connectionFactory" ref="jmsConnectionFactory" />
        <property name="destination" ref="platformLoaderQueue" />
        <property name="messageListener" ref="platformLoaderListener" />
        <property name="transactionManager" ref="transactionManager" />
    </bean>

    <bean id="transactionManager" class="org.springframework.orm.hibernate3.HibernateTransactionManager">
        <property name="sessionFactory">
            <ref local="sessionFactory"/>
        </property>
    </bean>
        
    <bean id="configurationHelper" class="gov.nih.nci.caintegrator2.common.ConfigurationHelperImpl">
        <property name="dataSource">
            <ref local="dataSource"/>
        </property>
    </bean>
    
    <tx:annotation-driven transaction-manager="transactionManager" />
    
    <bean id="sessionFactory" 
        class="org.springframework.orm.hibernate3.LocalSessionFactoryBean">
        <property name="dataSource">
            <ref local="dataSource"/>
        </property>
        <property name="configLocation">
            <value>classpath:hibernate.cfg.xml</value>
        </property>
    </bean>
    
    <bean id="dataSource" class="org.springframework.jndi.JndiObjectFactoryBean">
        <property name="jndiName" value="java:jdbc/CaIntegrator2DataSource" />
    </bean>

</beans>