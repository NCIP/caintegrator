<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.caintegrator2.domain.imaging">
	<class name="ImageSeriesAcquisition" table="IMAGE_SERIES_ACQUISITION" lazy="false" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="ID">
			<generator class="native" />
		</id>
		<!-- Attributes mapping for the ImageSeriesAcquisition class -->
        <property name="identifier" type="string" column="IDENTIFIER"/>
        <property name="nciaTrialIdentifier" type="string" column="NCIA_TRIAL_IDENTIFIER"/>
		<!-- Associations mapping for the ImageSeriesAcquisition class -->
		<many-to-one name="timepoint" class="gov.nih.nci.caintegrator2.domain.translational.Timepoint" column="TIMEPOINT_ID" lazy="proxy" cascade="save-update,merge" />
		<set name="seriesCollection" lazy="true" cascade="all" >
			<cache usage="read-write" />
			<key column="IMAGE_STUDY_ID" />
			<one-to-many class="gov.nih.nci.caintegrator2.domain.imaging.ImageSeries"/>
		</set>
		<many-to-one name="assignment" class="gov.nih.nci.caintegrator2.domain.translational.StudySubjectAssignment" column="STUDY_SUBJECT_ASSIGN_ID" lazy="proxy" cascade="save-update,merge" />
	</class>
</hibernate-mapping>