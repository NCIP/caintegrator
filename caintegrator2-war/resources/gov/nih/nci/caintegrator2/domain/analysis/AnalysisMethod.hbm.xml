<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.caintegrator2.domain.analysis">
	<class name="AnalysisMethod" table="ANALYSIS_METHOD" lazy="false" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="ID">
			<generator class="native" />
		</id>
		<!-- Attributes mapping for the AnalysisMethod class -->
		<property name="description" type="string" column="DESCRIPTION"/>
		<!-- Associations mapping for the AnalysisMethod class -->
	</class>
</hibernate-mapping>