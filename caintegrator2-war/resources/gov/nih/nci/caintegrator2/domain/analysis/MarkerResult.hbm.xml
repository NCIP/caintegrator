<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.caintegrator2.domain.analysis">
	<class name="MarkerResult" table="MARKER_RESULT" lazy="false" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="ID">
			<generator class="native" />
		</id>
		<!-- Attributes mapping for the Gene class -->
		<property name="rank" type="integer" column="RANK"/>
		<property name="feature" type="string" column="FEATURE"/>
		<property name="description" type="string" column="DESCRIPTION"/>
		<property name="score" type="double" column="SCORE"/>
		<property name="featureP" type="double" column="FEATURE_P"/>
		<property name="featurePLow" type="double" column="FEATURE_P_LOW"/>
		<property name="featurePHigh" type="double" column="FEATURE_P_HIGH"/>
		<property name="fdr" type="double" column="FDR"/>
		<property name="qvalue" type="double" column="Q_VALUE"/>
		<property name="bonferroni" type="double" column="BONFERRONI"/>
		<property name="maxT" type="double" column="MAX_T"/>
		<property name="fwer" type="double" column="FWER"/>
		<property name="foldChange" type="double" column="FOLD_CHANGE"/>
		<property name="class0Mean" type="double" column="CLASS_0_MEAN"/>
		<property name="class0Std" type="double" column="CLASS_0_STD"/>
		<property name="class1Mean" type="double" column="CLASS_1_MEAN"/>
		<property name="class1Std" type="double" column="CLASS_1_STD"/>
		<property name="k" type="double" column="K"/>
	</class>
</hibernate-mapping>