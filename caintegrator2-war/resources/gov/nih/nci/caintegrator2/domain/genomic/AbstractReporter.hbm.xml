<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.caintegrator2.domain.genomic">
	<class name="AbstractReporter" table="ABSTRACT_REPORTER" lazy="false" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="ID">
			<generator class="native" />
		</id>
		<!-- Attributes mapping for the AbstractReporter class -->
		<property name="name" type="string" column="NAME"/>
		<!-- Associations mapping for the AbstractReporter class -->
		<joined-subclass name="GeneExpressionReporter" table="GENE_EXPRESSION_REPORTER" lazy="false">
			<key column="ID" />
			<!-- Associations mapping for the GeneExpressionReporter class -->
			<many-to-one name="gene" class="gov.nih.nci.caintegrator2.domain.genomic.Gene" column="GENE_ID" lazy="proxy" cascade="save-update" />
		</joined-subclass>
	</class>
</hibernate-mapping>