<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.caintegrator2.domain.genomic">
	<class name="SampleAcquisition" table="SAMPLE_ACQUISITION" lazy="false" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="ID">
			<generator class="native" />
		</id>
		<!-- Attributes mapping for the SampleAcquisition class -->
		<!-- Associations mapping for the SampleAcquisition class -->
		<many-to-one name="timepoint" class="gov.nih.nci.caintegrator2.domain.translational.Timepoint" column="TIMEPOINT_ID" lazy="proxy" cascade="save-update,merge" />
		<many-to-one name="assignment" class="gov.nih.nci.caintegrator2.domain.translational.StudySubjectAssignment" column="STUDY_SUBJECT_ASSIGN_ID" lazy="proxy" cascade="save-update,merge"/>
		<many-to-one name="sample" class="gov.nih.nci.caintegrator2.domain.genomic.Sample" column="SAMPLE_ID"/>
		<set name="annotationCollection" lazy="true" cascade="all" >
			<cache usage="read-write" />
			<key column="SAMPLE_ACQUISITION_ID" />
			<one-to-many class="gov.nih.nci.caintegrator2.domain.annotation.AbstractAnnotationValue"/>
		</set>
	</class>
</hibernate-mapping>