<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.caintegrator2.domain.application">
	<class name="QueryResult" table="QUERY_RESULT" lazy="false" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="ID">
			<generator class="native" />
		</id>
		<!-- Attributes mapping for the QueryResult class -->
		<!-- Associations mapping for the QueryResult class -->
		<set name="rowCollection" lazy="true" cascade="save-update,merge" >
			<cache usage="read-write" />
			<key column="QUERY_RESULT_ID" />
			<one-to-many class="gov.nih.nci.caintegrator2.domain.application.ResultRow"/>
		</set>
		<many-to-one name="query" class="gov.nih.nci.caintegrator2.domain.application.Query" column="QUERY_ID" lazy="proxy" cascade="save-update,merge" />
	</class>
</hibernate-mapping>