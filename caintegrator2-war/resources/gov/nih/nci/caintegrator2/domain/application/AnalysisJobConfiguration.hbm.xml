<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.caintegrator2.domain.application">
	<class name="AnalysisJobConfiguration" table="ANALYSIS_JOB_CONFIG" lazy="false" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="ID">
			<generator class="native" />
		</id>
		<!-- Attributes mapping for the AnalysisJobConfiguration class -->
		<!-- Associations mapping for the AnalysisJobConfiguration class -->
		<many-to-one name="analysisMethod" class="gov.nih.nci.caintegrator2.domain.analysis.AnalysisMethod" column="ANALYSIS_METHOD_ID" lazy="proxy" cascade="save-update" />
		<many-to-one name="subscription" class="gov.nih.nci.caintegrator2.domain.application.StudySubscription" column="STUDY_SUBSCRIPTION_ID" lazy="proxy" cascade="save-update" />
	</class>
</hibernate-mapping>