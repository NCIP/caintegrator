<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.caintegrator2.domain.application">
    <typedef name="SortTypeEnum" class="gov.nih.nci.caintegrator2.common.GenericEnumUserType"> 
        <param name="enumClass">gov.nih.nci.caintegrator2.domain.application.SortTypeEnum</param> 
    </typedef> 
    <typedef name="EntityTypeEnum" class="gov.nih.nci.caintegrator2.common.GenericEnumUserType"> 
        <param name="enumClass">gov.nih.nci.caintegrator2.domain.application.EntityTypeEnum</param> 
    </typedef> 
	<class name="ResultColumn" table="RESULT_COLUMN" lazy="false" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="ID">
			<generator class="native" />
		</id>
		<!-- Attributes mapping for the ResultColumn class -->
		<property name="columnIndex" type="integer" column="COLUMN_INDEX"/>
		<property name="entityType" type="EntityTypeEnum" column="ENTITY_TYPE"/>
		<property name="sortOrder" type="integer" column="SORT_ORDER"/>
		<property name="sortType" type="SortTypeEnum" column="SORT_TYPE"/>
		<!-- Associations mapping for the ResultColumn class -->
		<many-to-one name="annotationDefinition" class="gov.nih.nci.caintegrator2.domain.annotation.AnnotationDefinition" column="ANNOTATION_DEFINITION_ID" lazy="proxy" cascade="none" />
		<many-to-one name="annotationFieldDescriptor" class="gov.nih.nci.caintegrator2.application.study.AnnotationFieldDescriptor" column="ANNOTATION_FIELD_DESCRIPTOR_ID" lazy="proxy" cascade="none" />
		
	</class>
</hibernate-mapping>