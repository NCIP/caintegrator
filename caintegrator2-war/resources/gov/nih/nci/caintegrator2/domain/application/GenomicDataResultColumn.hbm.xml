<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.caintegrator2.domain.application">
	<class name="GenomicDataResultColumn" table="GENOMIC_DATA_RESULT_COLUMN" lazy="false" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="ID">
			<generator class="native" />
		</id>
		<!-- Attributes mapping for the GenomicDataResultColumn class -->
		<property name="columnIndex" type="integer" column="COLUMN_INDEX"/>
		<!-- Associations mapping for the GenomicDataResultColumn class -->
		<many-to-one name="sampleAcquisition" class="gov.nih.nci.caintegrator2.domain.genomic.SampleAcquisition" column="SAMPLE_ACQUISITION_ID" lazy="proxy" cascade="save-update,merge" />
	</class>
</hibernate-mapping>