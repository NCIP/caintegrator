<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.caintegrator2.domain.annotation">
	<class name="AbstractPermissableValue" table="ABSTRACT_PERMISSABLE_VALUE" lazy="false" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="ID">
			<generator class="native" />
		</id>
		<!-- Attributes mapping for the AbstractPermissableValue class -->
		<!-- Associations mapping for the AbstractPermissableValue class -->
		<joined-subclass name="NumericPermissableValue" table="NUMERIC_PERMISSABLE_VALUE" lazy="false">
			<key column="ID" />
			<!-- Attributes mapping for the NumericPermissableValue class -->
			<property name="highValue" type="double" column="HIGH_VALUE"/>
			<property name="isRangeValue" type="integer" column="IS_RANGE_VALUE"/>
			<property name="lowValue" type="double" column="LOW_VALUE"/>
			<property name="numericValue" type="double" column="NUMERIC_VALUE"/>
		</joined-subclass>
		<joined-subclass name="StringPermissableValue" table="STRING_PERMISSABLE_VALUE" lazy="false">
			<key column="ID" />
			<!-- Attributes mapping for the StringPermissableValue class -->
			<property name="stringValue" type="string" column="STRING_VALUE"/>
		</joined-subclass>
		<joined-subclass name="DatePermissableValue" table="DATE_PERMISSABLE_VALUE" lazy="false">
			<key column="ID" />
			<!-- Attributes mapping for the DatePermissableValue class -->
			<property name="dateValue" type="java.util.Date" column="DATE_VALUE"/>
		</joined-subclass>
	</class>
</hibernate-mapping>