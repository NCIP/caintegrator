<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.caintegrator2.application.study">
    <class name="AuthorizedQuery" table="AUTHORIZED_QUERY" lazy="true" polymorphism="explicit">
        <cache usage="read-write" />
        <id name="id" type="long" column="ID">
            <generator class="native" />
        </id>
        
        <many-to-one name="authorizedStudyElementsGroup" 
            class="gov.nih.nci.caintegrator2.application.study.AuthorizedStudyElementsGroup" 
            column="AUTHORIZED_STUDY_ELEMENTS_GROUP_ID"
            not-null="true"
            insert="false"
            update="false"/>

        <many-to-one name="query"
            class="gov.nih.nci.caintegrator2.domain.application.Query"
            column="QUERY_ID"
            lazy="proxy"
            cascade="all" />
            
    </class>
</hibernate-mapping>