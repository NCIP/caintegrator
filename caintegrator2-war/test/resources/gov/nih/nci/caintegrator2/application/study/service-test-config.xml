<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-lazy-init="false" default-dependency-check="none" default-autowire="no">

    <bean id="studyManagementService" 
    class="gov.nih.nci.caintegrator2.application.study.StudyManagementServiceImpl" singleton="true">
        <property name="dao">
            <ref local="dao"/>
        </property>
        <property name="fileManager">
            <ref local="fileManagerStub"/>
        </property>
        <property name="caDSRFacade">
            <ref local="caDSRFacadeStub"/>
        </property>
        <property name="caArrayFacade">
            <ref local="caArrayFacadeStub"/>
        </property>
        <property name="nciaFacade">
            <ref local="nciaFacadeStub"/>
        </property>
    </bean>
    
    <bean id="dao" class="gov.nih.nci.caintegrator2.data.CaIntegrator2DaoStub" />
    
    <bean id="fileManagerStub" class="gov.nih.nci.caintegrator2.file.FileManagerStub" />

    <bean id="fileManager"
        class="gov.nih.nci.caintegrator2.file.FileManagerImpl">
        <property name="configurationHelper">
            <ref local="configurationHelperStub" />
        </property>
    </bean>
        
    <bean id="configurationHelperStub" class="gov.nih.nci.caintegrator2.common.ConfigurationHelperStub" />

    <bean id="caDSRFacadeStub"
        class="gov.nih.nci.caintegrator2.external.cadsr.CaDSRFacadeStub" />

    <bean id="caArrayFacadeStub"
        class="gov.nih.nci.caintegrator2.external.caarray.CaArrayFacadeStub" />

    <bean id="nciaFacadeStub"
        class="gov.nih.nci.caintegrator2.external.ncia.NCIAFacadeStub" />

</beans>