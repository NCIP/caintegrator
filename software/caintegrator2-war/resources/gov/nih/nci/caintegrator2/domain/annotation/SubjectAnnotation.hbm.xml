<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.caintegrator2.domain.annotation">
	<class name="SubjectAnnotation" table="SUBJECT_ANNOTATION" lazy="false" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="ID">
			<generator class="native" />
		</id>
		<!-- Attributes mapping for the SubjectAnnotation class -->
		<!-- Associations mapping for the SubjectAnnotation class -->
		<many-to-one name="studySubjectAssignment" class="gov.nih.nci.caintegrator2.domain.translational.StudySubjectAssignment" column="STUDY_SUBJECT_ASSIGNMENT_ID" cascade="save-update,merge" />
		<many-to-one name="annotationValue" class="gov.nih.nci.caintegrator2.domain.annotation.AbstractAnnotationValue" column="ANNOTATION_VALUE_ID" unique="true" cascade="all"/>
		<many-to-one name="timepoint" class="gov.nih.nci.caintegrator2.domain.translational.Timepoint" column="TIMEPOINT_ID" cascade="all" />
	</class>
</hibernate-mapping>