<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.caintegrator2.domain.application">
    <typedef name="GenePatternJobStatusEnum" class="gov.nih.nci.caintegrator2.common.GenericEnumUserType"> 
       <param name="enumClass">gov.nih.nci.caintegrator2.domain.application.GenePatternJobStatusEnum</param> 
    </typedef> 
	<class name="GenePatternAnalysisJob" table="GENE_PATTERN_ANALYSIS_JOB" lazy="false" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="ID">
			<generator class="native" />
		</id>
		<!-- Attributes mapping for the Query class -->
		<property name="name" type="string" column="NAME"/>
        <property name="status" type="GenePatternJobStatusEnum" column="STATUS"/>
        <property name="jobUrl" type="string" column="JOB_URL"/>
		<property name="creationDate" type="java.util.Date" column="CREATION_DATE"/>
		<property name="gpJobNumber" type="integer" column="GP_JOB_NUMBER"/>
		<!-- Associations mapping for the Query class -->
		<many-to-one name="subscription" class="gov.nih.nci.caintegrator2.domain.application.StudySubscription" column="STUDY_SUBSCRIPTION_ID" lazy="proxy" cascade="none" />
        
	</class>
</hibernate-mapping>