<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.caintegrator2.domain.annotation">
	<class name="CommonDataElement" table="COMMON_DATA_ELEMENT" lazy="false" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="long" column="ID">
			<generator class="native" />
		</id>
		<!-- Attributes mapping for the CommonDataElement class -->
		<property name="publicID" type="long" column="PUBLIC_ID"/>
		<property name="version" type="string" column="VERSION"/>
		<property name="longName" type="string" column="LONG_NAME"/>
		<property name="preferredName" type="string" column="PREFERRED_NAME"/>
		<property name="contextName" type="string" column="CONTEXT_NAME"/>
		<property name="definition" type="string" column="DEFINITION"/>
		<property name="registrationStatus" type="string" column="REGISTRATION_STATUS"/>
        <property name="type" type="string" column="TYPE"/>
        <property name="workflowStatus" type="string" column="WORKFLOW_STATUS"/>
        
		<!-- Associations mapping for the CommonDataElement class -->
	</class>
</hibernate-mapping>