<?xml version="1.0" encoding="UTF-8"?>

<ivy-module version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://incubator.apache.org/ivy/schemas/ivy.xsd">

  <info organisation="caGrid" module="dataExtensions" revision="1.2" status="release" publication="20080324163748">
    <license name="caGrid" url="http://www.cagrid.org/wiki/CaGrid:LICENSE-1.2"/>
    <description>Data Service Introduce Extensions</description>
  </info>

  <configurations>
    <!-- Private Configurations-->
    <conf name="target_grid" visibility="private"/>

    <conf name="impl" visibility="private" extends="styles"/>
    <conf name="impl-test" visibility="private"/>

    <conf name="sdk31" visibility="private"/>
    <conf name="sdk32" visibility="private"/>
    <conf name="authz" visibility="private"/>

    <!-- Public Configurations-->
    <conf name="core" visibility="public"/>
    <conf name="ui" visibility="public"/>
    <conf name="default" visibility="public" extends="core,ui"/>

    <conf name="style-cacore31" extends="default" visibility="public"/>
    <conf name="style-cacore32" extends="default" visibility="public"/>
    <conf name="styles" extends="style-cacore31,style-cacore32" visibility="public"/>

    <conf name="test" visibility="public" extends="styles,impl-test"/>
  </configurations>


  <publications>
    <artifact name="caGrid-dataExtensions-core" conf="core" type="jar"/>
    <artifact name="caGrid-dataExtensions-ui" conf="ui" type="jar"/>
    <artifact name="caGrid-dataExtensions-upgrades" conf="core" type="jar"/>
    <artifact name="caGrid-dataExtensions-sdkstyle" conf="default" type="jar"/>

    <artifact name="caGrid-dataExtensions-style-cacore31" conf="style-cacore31" type="jar"/>
    <artifact name="caGrid-dataExtensions-style-cacore32" conf="style-cacore32" type="jar"/>

    <artifact name="caGrid-dataExtensions-tests" conf="test" type="jar"/>
    <artifact name="caGrid-dataExtensions-style-tests-cacore31" conf="test" type="jar"/>
    <artifact name="caGrid-dataExtensions-style-tests-cacore32" conf="test" type="jar"/>
  </publications>

  <dependencies>
    <dependency rev="nci_prod-1.2" org="caGrid" name="target_grid" conf="target_grid->urls"/>

    <dependency rev="1.2" org="caGrid" name="core" conf="core->default;ui->ui;impl-test->test"/>
    <dependency rev="1.2" org="caGrid" name="introduce" conf="core->extension;impl-test->extension-test"/>
    <dependency rev="1.2" org="caGrid" name="data" conf="core->full,service-schemas;impl-test->test"/>
    <dependency rev="1.2" org="caGrid" name="gme" conf="ui->client"/>
    <dependency rev="1.2" org="caGrid" name="cadsr" conf="core->client;ui->portal"/>
    <dependency rev="1.2" org="caGrid" name="wsEnum" conf="core->default"/>
    <!-- NOTE: just needs this for a constant, so not using transitive dependency-->
    <dependency transitive="false" rev="1.2" org="caGrid" name="bulkDataTransfer" conf="core->service"/>
    <dependency rev="1.2" org="caGrid" name="metadata" conf="core->data.jars,schemas"/>
    <dependency rev="1.2" org="caGrid" name="metadatautils" conf="core->default"/>

    <dependency rev="1.2" org="caGrid" name="sdkQuery" conf="sdk31->default"/>
    <dependency rev="1.2" org="caGrid" name="sdkQuery32" conf="sdk32->default"/>
    
    <dependency rev="1.2" org="caGrid" name="authz" conf="authz->default"/>

    <!-- Extenernal Dependencies -->
    <dependency rev="1.0" org="wannheden" name="jetc-task" conf="impl->*"/>
    <dependency rev="2.0.0" org="jgoodies" name="validation" conf="ui->default"/>

  </dependencies>
</ivy-module>
