<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-lazy-init="false" default-dependency-check="none" default-autowire="no">
    <bean name="nciaFacadeIntegration" class="gov.nih.nci.caintegrator.external.ncia.NCIAFacadeImpl" singleton="true">
        <property name="nciaServiceFactory">
            <ref local="nciaServiceFactoryIntegration"/>
        </property>
        <property name="fileManager">
            <ref local="fileManager"/>
        </property>
        <property name="nciaDicomJobFactory">
            <ref local="nciaDicomJobFactoryIntegration"/>
        </property>    
    </bean>
    
    <bean id="fileManager" class="gov.nih.nci.caintegrator.file.FileManagerImpl">
        <property name="configurationHelper">
            <ref local="configurationHelperStub" />
        </property>
    </bean>
        
    <bean id="configurationHelperStub" class="gov.nih.nci.caintegrator.common.ConfigurationHelperStub" />
    
    <bean id="nciaServiceFactoryIntegration" class="gov.nih.nci.caintegrator.external.ncia.NCIAServiceFactoryImpl" />
        
    <bean id="nciaDicomJobFactoryIntegration" class="gov.nih.nci.caintegrator.external.ncia.NCIADicomJobFactoryImpl" />

    <bean id="nciaServerConnectionProfile" class="gov.nih.nci.caintegrator.external.ServerConnectionProfile" >
       <property name="url" value="http://imaging.nci.nih.gov/wsrf/services/cagrid/NCIACoreService" />
    </bean>
 
     <bean id="nciaProdGridServerConnectionProfile" class="gov.nih.nci.caintegrator.external.ServerConnectionProfile" >
       <property name="url" value="http://nciavgridprod5004.nci.nih.gov:39280/wsrf/services/cagrid/NCIACoreService" />
    </bean>   
    
    <bean id="nciaDevServerConnectionProfile" class="gov.nih.nci.caintegrator.external.ServerConnectionProfile" >
       <!--  Note, the dev url below can only be used when behind NCI's VPN  -->
       <property name="url" value="http://imaging-dev.nci.nih.gov/wsrf/services/cagrid/NCIACoreService" />
    </bean>
    
    <bean id="nciaStageServerConnectionProfile" class="gov.nih.nci.caintegrator.external.ServerConnectionProfile" >
       <property name="url" value="http://imaging-stage.nci.nih.gov/wsrf/services/cagrid/NCIACoreService" />
    </bean>
    
    <bean id="nciaQaServerConnectionProfile" class="gov.nih.nci.caintegrator.external.ServerConnectionProfile" >
       <property name="url" value="http://imaging-qa.nci.nih.gov/wsrf/services/cagrid/NCIACoreService" />
    </bean>   

    <bean id="nciaQaDirectConnectionProfile" class="gov.nih.nci.caintegrator.external.ServerConnectionProfile" >
       <property name="url" value="http://nciavgridqa5004.nci.nih.gov:39280/wsrf/services/cagrid/NCIACoreService" />
    </bean>   
     
    <bean id="nciaNiamsServerConnectionProfile" class="gov.nih.nci.caintegrator.external.ServerConnectionProfile" >
       <property name="url" value="http://ncias-s202-v.nci.nih.gov:39280/wsrf/services/cagrid/NCIACoreService" />
    </bean>    
   
</beans>